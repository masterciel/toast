{
  "name": "vue-toastification",
  "version": "1.7.7",
  "private": false,
  "description": "Toasts for Vue made easy!",
  "author": "Gustavo Maronato",
  "scripts": {
    "dev": "vite serve docs",
    "prebuild": "rimraf ./dist",
    "build": "rollup -c build/rollup.config.js",
    "build:demo": "vite build --outDir demo docs",
    "test:unit": "vue-cli-service test:unit",
    "test": "yarn test:unit",
    "test:watch": "yarn test --watch",
    "lint": "yarn lint:no-fix --fix",
    "lint:no-fix": "eslint --ext .js,.vue,.ts ./src/",
    "prepublishOnly": "yarn lint:no-fix && yarn test && yarn build"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "browser": {
    "./dist/cjs/index.js": "./dist/umd/index.js",
    "./dist/esm/index.js": "./dist/esm/index.js"
  },
  "unpkg": "dist/umd/index.min.js",
  "jsdelivr": "dist/umd/index.min.js",
  "typings": "dist/types/src/index.d.ts",
  "types": "dist/types/src/index.d.ts",
  "files": [
    "dist",
    "src",
    "nuxt",
    "composition"
  ],
  "style": "dist/index.css",
  "dependencies": {},
  "devDependencies": {
    "@mdi/font": "^5.3.45",
    "@nuxt/types": "^2.14.5",
    "@types/jest": "^26.0.13",
    "@types/vue": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "@typescript-eslint/parser": "^4.1.0",
    "@vue/compiler-sfc": "^3.0.0-rc.10",
    "@vue/test-utils": "^2.0.0-beta.4",
    "babel-eslint": "^10.1.0",
    "core-js": "^3.6.5",
    "deepmerge": "^4.2.2",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.0-beta.3",
    "fibers": "^5.0.0",
    "husky": "^4.3.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^10.3.0",
    "nuxt": "^2.12.2",
    "prettier": "^2.0.5",
    "prismjs": "^1.20.0",
    "rollup": "^2.11.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^9.0.0",
    "rollup-plugin-sass": "^1.2.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "rollup-plugin-vue": "^6.0.0-beta.10",
    "sass": "^1.26.5",
    "sass-loader": "^10.0.2",
    "typescript": "^4.0.2",
    "vite": "^1.0.0-rc.4",
    "vue": "^3.0.0-rc.10",
    "vue-prism-component": "^1.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue,ts,tsx}": [
      "yarn lint:no-fix",
      "git add"
    ]
  },
  "peerDependencies": {
    "vue": "^2.0.0"
  },
  "bugs": {
    "url": "https://github.com/Maronato/vue-toastification/issues"
  },
  "homepage": "https://github.com/Maronato/vue-toastification#readme",
  "keywords": [
    "vue",
    "notification",
    "toast"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Maronato/vue-toastification.git"
  }
}
